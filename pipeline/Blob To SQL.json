{
	"name": "Blob To SQL",
	"properties": {
		"activities": [
			{
				"name": "Blob TO SQL",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "xmlinputs"
					},
					{
						"name": "Destination",
						"value": "dbo.APIDATA"
					}
				],
				"typeProperties": {
					"source": {
						"type": "XmlSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"wildcardFileName": "*xml",
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "XmlReadSettings",
							"validationMode": "none",
							"detectDataType": false,
							"namespaces": true
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false,
						"tableOption": "autoCreate",
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"dataIntegrationUnits": 2,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['SessionListResponse']['ResponseStatus']['d2p1:ErrorCode']['i:@nil']"
								}
							},
							{
								"source": {
									"path": "$['SessionListResponse']['ResponseStatus']['d2p1:Message']['i:@nil']"
								}
							},
							{
								"source": {
									"path": "$['SessionListResponse']['ResponseStatus']['d2p1:StackTrace']['i:@nil']"
								}
							},
							{
								"source": {
									"path": "$['SessionListResponse']['ResponseStatus']['d2p1:Errors']['i:@nil']"
								}
							},
							{
								"source": {
									"path": "$['SessionListResponse']['ResponseStatus']['d2p1:Meta']['i:@nil']"
								}
							},
							{
								"source": {
									"path": "$['SessionListResponse']['Sessions']['SessionRecord'][0]['Application']"
								},
								"sink": {
									"name": "Application",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['SessionListResponse']['Sessions']['SessionRecord'][0]['BytesReceived']"
								},
								"sink": {
									"name": "BytesReceived",
									"type": "Int32"
								}
							},
							{
								"source": {
									"path": "$['SessionListResponse']['Sessions']['SessionRecord'][0]['BytesSent']"
								},
								"sink": {
									"name": "BytesSent",
									"type": "Int32"
								}
							},
							{
								"source": {
									"path": "$['SessionListResponse']['Sessions']['SessionRecord'][0]['Client']"
								},
								"sink": {
									"name": "Client",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['SessionListResponse']['Sessions']['SessionRecord'][0]['ClientGrid']"
								},
								"sink": {
									"name": "ClientGrid",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['SessionListResponse']['Sessions']['SessionRecord'][0]['Cms']"
								},
								"sink": {
									"name": "Cms",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['SessionListResponse']['Sessions']['SessionRecord'][0]['Degrees']"
								},
								"sink": {
									"name": "Degrees",
									"type": "Int32"
								}
							},
							{
								"source": {
									"path": "$['SessionListResponse']['Sessions']['SessionRecord'][0]['Frequency']"
								},
								"sink": {
									"name": "Frequency",
									"type": "Int32"
								}
							},
							{
								"source": {
									"path": "$['SessionListResponse']['Sessions']['SessionRecord'][0]['HoldingSeconds']"
								},
								"sink": {
									"name": "HoldingSeconds",
									"type": "Int32"
								}
							},
							{
								"source": {
									"path": "$['SessionListResponse']['Sessions']['SessionRecord'][0]['IdTag']"
								},
								"sink": {
									"name": "IdTag",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['SessionListResponse']['Sessions']['SessionRecord'][0]['Kilometers']"
								},
								"sink": {
									"name": "Kilometers",
									"type": "Int32"
								}
							},
							{
								"source": {
									"path": "$['SessionListResponse']['Sessions']['SessionRecord'][0]['LastCommand']"
								},
								"sink": {
									"name": "LastCommand",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['SessionListResponse']['Sessions']['SessionRecord'][0]['MessagesReceived']"
								},
								"sink": {
									"name": "MessagesReceived",
									"type": "Int32"
								}
							},
							{
								"source": {
									"path": "$['SessionListResponse']['Sessions']['SessionRecord'][0]['MessagesSent']"
								},
								"sink": {
									"name": "MessagesSent",
									"type": "Int32"
								}
							},
							{
								"source": {
									"path": "$['SessionListResponse']['Sessions']['SessionRecord'][0]['Mode']"
								},
								"sink": {
									"name": "Mode",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['SessionListResponse']['Sessions']['SessionRecord'][0]['Server']"
								},
								"sink": {
									"name": "Server",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['SessionListResponse']['Sessions']['SessionRecord'][0]['ServerGrid']"
								},
								"sink": {
									"name": "ServerGrid",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['SessionListResponse']['Sessions']['SessionRecord'][0]['Sid']"
								},
								"sink": {
									"name": "Sid",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['SessionListResponse']['Sessions']['SessionRecord'][0]['Timestamp']"
								},
								"sink": {
									"name": "Timestamp",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['SessionListResponse']['Sessions']['SessionRecord'][0]['Version']"
								},
								"sink": {
									"name": "Version",
									"type": "String"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "xml_blob_connector",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "APITable",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}